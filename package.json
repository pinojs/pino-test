{
  "name": "pino-test",
  "version": "1.0.0",
  "description": "A set of API to easily test the Pino logger",
  "main": "pino-test.js",
  "type": "commonjs",
  "types": "pino-test.d.ts",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && node --experimental-test-coverage --test-reporter=spec --test **/*.test.js && jest test/jest && npm run test-types",
    "test-ci": "npm run lint && node --experimental-test-coverage --test-reporter=lcov --test **/*.test.js && jest test/jest && npm run test-types",
    "test-ci-pnpm": "pnpm run lint && node --experimental-test-coverage --test-reporter=lcov --test **/*.test.js && pnpm run test-types",
    "test-ci-yarn-pnp": "yarn run lint && node --experimental-test-coverage --test-reporter=lcov --test **/*.test.js",
    "test-types": "tsc && tsd && ts-node test/types/pino-test.ts"
  },
  "precommit": "test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinojs/pino-test.git"
  },
  "keywords": [
    "pino",
    "logger",
    "test",
    "stream"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinojs/pino-test/issues"
  },
  "homepage": "https://github.com/pinojs/pino-test#readme",
  "dependencies": {
    "split2": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "pino": "^8.19.0",
    "pre-commit": "^1.2.2",
    "ts-node": "^10.9.2",
    "tsd": "^0.30.7",
    "typescript": "^5.4.2"
  },
  "tsd": {
    "directory": "test/types"
  }
}
